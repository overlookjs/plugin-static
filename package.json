{
  "name": "@overlook/plugin-static",
  "version": "0.2.0",
  "description": "Overlook framework static file plugin",
  "main": "index.js",
  "files": [
    "lib/**/*.js"
  ],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookjs/plugin-static.git"
  },
  "bugs": {
    "url": "https://github.com/overlookjs/plugin-static/issues"
  },
  "dependencies": {
    "@overlook/plugin": "^0.2.6",
    "@overlook/plugin-match": "^0.8.0",
    "@overlook/plugin-serve-http": "^0.3.2",
    "@overlook/route": "^0.6.2",
    "express": "^4.17.1",
    "is-it-type": "^4.0.0",
    "simple-invariant": "^2.0.0"
  },
  "devDependencies": {
    "@overlook/plugin-path": "^0.8.0",
    "@overlook/plugin-request": "^0.1.1",
    "@overlookmotel/eslint-config": "^7.1.2",
    "@overlookmotel/eslint-config-jest": "^4.0.1",
    "@overlookmotel/eslint-config-node": "^2.0.0",
    "axios": "^0.19.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5"
  },
  "keywords": [
    "overlook",
    "overlook-plugin",
    "static",
    "file"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js'",
    "lint-fix": "eslint '*.js' '.*.js' '**/*.js' '**/.*.js' --fix",
    "test-main": "jest",
    "test-prod": "cross-env NODE_ENV=production jest",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=10"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
